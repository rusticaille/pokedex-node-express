<%- include('partials/header') %>

    <main class="home_container">
 
    <% pokemons.forEach(pokemon => { %>
     
        <article class="pokemon_item">
            <a class="pokemon_link" href="/pokemon/<%= pokemon.numero%>">
                <img src="/img/<%= pokemon.numero%>.png" alt="<%= pokemon.nom%>">
                <h2 class="pokemon_title">#<%= pokemon.numero%> <%= pokemon.nom%></h2>
            </a>
        </article>

    <% }) %>

    </main>
</body>
</html>